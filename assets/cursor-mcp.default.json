{
	"mcpServers": {
		"context7": {
			"command": "npx",
			"args": [
				"-y",
				"@upstash/context7-mcp@latest"
			]
		},
		"serena": {
			"command": "uvx",
			"args": [
				"--from",
				"git+https://github.com/oraios/serena",
				"serena",
				"start-mcp-server",
				"--context",
				"ide-assistant"
			]
		},
		"filesystem": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-filesystem",
				"__CURSOR_WORKSPACE_DIR__"
			]
		},
		"mcp-atlassian-api": {
			"command": "npx",
			"args": [
				"mcp-remote",
				"https://mcp.atlassian.com/v1/mcp"
			]
		}
	}
}
